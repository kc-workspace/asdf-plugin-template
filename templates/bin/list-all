#!/usr/bin/env bash

## List all installable versions.
## https://asdf-vm.com/plugins/create.html#bin-list-all

set -euo pipefail

## Environment variables
## https://asdf-vm.com/plugins/create.html#environment-variables-overview

## the path the plugin was installed
plugin_path="${ASDF_PLUGIN_PATH:?}"

## Utility scripts
## https://asdf-vm.com/plugins/create.html#extension-commands-for-asdf-cli

# shellcheck source=../lib/utils.sh
source "${plugin_path}/lib/utils.sh"

## -----------------------------------------------------------------------

asdf_sorted_version
